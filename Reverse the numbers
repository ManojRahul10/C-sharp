using System;
class Program
{
    static void Main()
    {
        int a = int.Parse(Console.ReadLine());
        int reversed = 0;
        while (a != 0)
        {
            int x = a % 10;
            reversed = reversed * 10 + x;
            int y = a / 10;
            a = y;
            
            
        }
        Console.WriteLine(reversed);
    }
}

